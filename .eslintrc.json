{
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },

    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "rules": {
      "no-unsafe-finally": "off",
      "no-unused-vars": "off",
      "no-use-before-define": [2, { "functions": false ,  "classes": false, "variables": false}],
      "no-console": "off"
    },
    "globals": {
      "globalThis": true
    },
    "extends": "eslint:recommended"
}
